{
    "builds": [{
        "src": "JPR_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime":"python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "JPR_management_system/wsgi.py"
        }
    ]
}